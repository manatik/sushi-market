{
  "trailingComma": "none",
  "useTabs": true,
  "printWidth": 120,
  "semi": false,
  "tabWidth": 2,
  "jsxSingleQuote": true,
  "singleQuote": true,
  "arrowParens": "avoid",
  "importOrder": [
    "<THIRD_PARTY_MODULES>",
    "^@components/(.*)$",
    "^@providers/(.*)$",
    "^@services/(.*)$",
    "^@query-hooks/(.*)$",
    "^@assets/(.*)$",
    "^@common-types/(.*)$",
    "^@hooks/(.*)$",
    "^@store/(.*)$",
    "^@utils/(.*)$",
    "^../(.*)",
    "^./(.*)",
    "(.scss)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}